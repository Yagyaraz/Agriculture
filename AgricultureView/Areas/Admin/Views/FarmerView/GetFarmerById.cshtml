    <script>
        $(document).ready(function () {
            $.ajax({
                url: '@Url.Action("GetAllActiveAgriculture", "FarmerView")',
                success: function (result) {
                    const sections = {
                        1: '#Gaipalan',
                        2: '#Bhaisipalan',
                        3: '#Chauri',
                        4: '#Goru',
                        5: '#Beda',
                        6: '#Bungur',
                        7: '#Hen',
                        8: '#otherBird',
                        9: '#Fish',
                        10: '#otherPashu',
                        11: '#Grass',
                        12: '#farmDet',
                        13: '#meachineDet',
                        14: '#sanrachDet'
                    };

                    result.forEach(function (item) {
                        const sectionId = sections[item.value];
                        if (sectionId) {
                            $(sectionId).show();
                        }
                    });
                }
            });
        });

    </script>
