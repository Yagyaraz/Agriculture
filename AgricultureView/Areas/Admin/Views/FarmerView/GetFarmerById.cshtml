<div class="row">
    <div class="card">
        <div class="card-header">
            <div class="card-title">Basic Information</div>
        </div>

        <div class="card-body">
            <div class="row">



                <div class="col-md-3">
                    <label class="fw-bold">जग्गा आफ्नो छ</label>
                    <p>@(Model.krishi.IsSelfJagga ? "हो" : "होइन")</p>
                </div>

                <div class="col-md-3">
                    <label class="fw-bold">कृषि आम्दानी</label>
                    <p>रु. @Model.krishi.IncomeFromAgri</p>
                </div>

                <div class="col-md-3">
                    <label class="fw-bold">गैर कृषि आम्दानी</label>
                    <p>रु. @Model.krishi.IncomeFromNonAgri</p>
                </div>

                <div class="col-md-3">
                    <label class="fw-bold">कुल आम्दानी</label>
                    <p>रु. @Model.krishi.TotalIncome</p>
                </div>

                <div class="col-md-3">
                    <label class="fw-bold">संलग्न समय</label>
                    <p>@Model.krishi.AvgMonthForAgriId</p>
                </div>

            </div>
        </div>
    </div>

    <!-- Agriculture Detail Table -->
    <div class="card mt-3">
        <div class="card-header">
            <div class="card-title">Agriculture Area Details</div>
        </div>

        <div class="card-body">
            <div class="table-responsive">
                <table class="table table-bordered table-hover">
                    <thead>
                        <tr>
                            <th>वडा नं</th>
                            <th>गाउँ</th>
                            <th>क्षेत्र</th>
                            <th>उप-क्षेत्र</th>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var item in Model.krishi.AgricultureDetailViewModelList)
                        {
                            <tr>
                                <td>@item.AgriWardNo</td>
                                <td>@item.AgriAddress</td>
                                <td>@item.AgriSectorId</td>
                                <td>@item.AgriSubSector</td>
                            </tr>
                        }
                    </tbody>
                </table>
            </div>
        </div>
    </div>

    <!-- Land Area Section -->
    @if (Model.krishi.IsSelfJagga)
    {
        <div class="card mt-3">
            <div class="card-header">
                <div class="card-title">Land Area Detail</div>
            </div>
            <div class="card-body">

                @if (Model.krishi.ChooseLand == "")
                {
                    <p>
                        <strong>रोपनी:</strong> @Model.krishi.TotalRopani ,
                        <strong>आना:</strong> @Model.krishi.TotalAana ,
                        <strong>पैसा:</strong> @Model.krishi.TotalPaisa ,
                        <strong>दाम:</strong> @Model.krishi.TotalDam <br />
                        <strong>वर्ग मिटर:</strong> @Model.krishi.TotalRopaniSquareMiter
                    </p>
                }
                else if (Model.krishi.ChooseLand == "")
                {
                    <p>
                        <strong>बिगाह:</strong> @Model.krishi.TotalBigaha ,
                        <strong>कट्ठा:</strong> @Model.krishi.TotalKattha ,
                        <strong>धुर:</strong> @Model.krishi.TotalDhur <br />
                        <strong>वर्ग मिटर:</strong> @Model.krishi.TotalBigaSquareMiter
                    </p>
                }

            </div>
        </div>
    }
</div>

